{
  "name": "antara-hn-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for day-to-day HN tasks",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "bundle": "webpack",
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "start": "node server.js",
    "dev": "webpack-dev-server --mode development --open --hot --port 5000",
    "lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prebuild": "npm run lint && npm run format && npm run test",
    "build": "webpack --mode production",
    "check-types": "tsc"
  },
  "author": "Antara Health",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@babel/preset-typescript": "^7.9.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "~7.2.0",
    "eslint-config-airbnb-typescript-prettier": "^2.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@antarahealth/comms-ui": "^1.1.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "heroku-ssl-redirect": "^0.0.4",
    "history": "^5.0.0",
    "kremling": "^2.0.4",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.20",
    "react-router-dom": "^5.2.0",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
