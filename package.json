{
  "name": "scribe2",
  "version": "3.0.1",
  "description": "Dashboard for day-to-day HN tasks",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "url": "https://github.com/antarahealth/hn-dashboard",
    "type": "git"
  },
  "scripts": {
    "bundle": "webpack",
    "test": "jest --verbose --setupFiles dotenv/config",
    "test:watch": "jest --watch --verbose --setupFiles dotenv/config",
    "start": "node server.js",
    "dev": "npm run tailwind:dev & webpack-dev-server --mode development --port 5000",
    "lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prebuild": "npm run tailwind:prod && npm run lint && npm run format && npm run test",
    "build": "webpack --mode production",
    "check-types": "tsc",
    "heroku-prebuild": "node auth-npm.js",
    "release": "release-it",
    "release:dry": "release-it --dry-run",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "tailwind:dev": "npx tailwindcss -i ./src/styles/tailwind.config.css -o ./src/styles/tailwind.styles.css --watch",
    "tailwind:prod": "npx tailwindcss -i ./src/styles/tailwind.config.css -o ./src/styles/tailwind.styles.css --minify"
  },
  "author": "Antara Health",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.0",
    "@sentry/webpack-plugin": "^1.20.0",
    "@svgr/webpack": "^6.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/jest": "^29.2.0",
    "@types/markdown-draft-js": "^2.2.4",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-draft-wysiwyg": "^1.13.4",
    "@types/react-pdf": "^5.7.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/throttle-debounce": "^5.0.0",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "assert": "^2.0.0",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.0.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "eslint": "~8.26.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
    "react-test-renderer": "^18.2.0",
    "release-it": "^15.5.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "url": "^0.11.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@airtable/blocks": "1.15.0",
    "@analytics/segment": "^1.1.3",
    "@apollo/client": "^3.7.1",
    "@emotion/core": "^10.1.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^10.3.0",
    "@fullcalendar/core": "^6.1.4",
    "@fullcalendar/react": "^6.1.4",
    "@fullcalendar/resource": "^6.1.4",
    "@fullcalendar/resource-timeline": "^6.1.4",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.105",
    "@mui/material": "^5.10.12",
    "@mui/utils": "^5.10.9",
    "@mui/x-data-grid": "^5.17.8",
    "@mui/x-date-pickers": "^5.0.5",
    "@sentry/react": "^7.31.1",
    "@sentry/tracing": "^7.30.0",
    "ajv": "^8.11.0",
    "analytics": "^0.8.1",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.1.3",
    "browserify-zlib": "^0.2.0",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.6",
    "downshift": "^7.0.1",
    "draft-js": "^0.11.7",
    "express": "^4.18.2",
    "firebase": "9.13.0",
    "formik": "^2.2.9",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "heroku-ssl-redirect": "^0.1.1",
    "history": "^5.3.0",
    "html-react-parser": "^3.0.4",
    "jwt-decode": "^3.1.2",
    "kremling": "^2.1.0",
    "lodash": "^4.17.21",
    "markdown-draft-js": "^2.4.0",
    "mime-types": "^2.1.35",
    "mock-apollo-client": "^1.2.1",
    "mui-daterange-picker": "^1.0.5",
    "node-fetch": "^2.6.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-draggable": "^4.4.5",
    "react-feather": "^2.0.10",
    "react-google-login": "^5.2.2",
    "react-google-places-autocomplete": "^3.4.0",
    "react-hook-form": "^7.38.0",
    "react-image-render": "^0.0.3",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.4.2",
    "react-tippy": "^1.4.0",
    "react-toast-notifications": "^2.5.1",
    "secure-ls": "^1.2.6",
    "stream": "^0.0.2",
    "sweetalert2": "^11.6.4",
    "sweetalert2-react-content": "^5.0.7",
    "tableau-react": "^2.2.0",
    "throttle-debounce": "^5.0.0",
    "use-file-picker": "^1.5.1",
    "util": "^0.12.5",
    "webpack-bundle-analyzer": "^4.7.0",
    "yup": "^0.32.11"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styles.mock.js"
    },
    "globalSetup": "./jest.global.js",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "npx pretty-quick",
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
